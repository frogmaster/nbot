{
	"modules": [
                {
                "description": "Gets title from URL in text",
                "expression": "http",
                "command": "URL-detection",
                "module": "get_title"
                },
                {
                "description": "Help",
                "expression": "^!help",
                "command": "!help <module>",
                "module": "get_help"
                },
                {
                "description": "Usage: !ilm <location>\nFetches weather info for location, default Tallinn",
                "expression": "^!ilm.*",
		"command": "!ilm",
                "module": "get_weather"
                }
	]
}
