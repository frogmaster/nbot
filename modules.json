{
	"modules": [
                {
                "description": "Gets title from URL in text",
                "expression": "http",
                "command": "URL-detection",
                "module": "get_title"
                },
                {
                "description": "Shows module usage and purpose",
                "expression": "^!help",
                "command": "!help <module>",
                "module": "get_help"
                },
                {
                "description": "Lists all available modules",
                "expression": "^!modules$",
                "command": "!modules",
                "module": "get_modules"
                },
                {
                "description": "Usage: !ilm <location>\nFetches weather info for location, default Tallinn",
                "expression": "^!ilm.*",
		"command": "!ilm",
                "module": "get_weather"
                },
                {
                "description": "Fetches random boob URL",
                "expression": "^!boobs$",
                "command": "!boobs",
                "module": "get_boobs"
                },
                {
                "description": "Small betting engine. Usage: !when create betname, !when show betname, !when bet 2016-10-06 betname",
                "expression": "^!when.*",
                "command": "!when",
                "module": "get_when"
                }
	]
}
